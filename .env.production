# Production Environment Variables for Vercel Deployment
VITE_APP_NAME=HigherUp.ai
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=production

# Supabase Configuration (Replace with your actual values)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# API Configuration
VITE_API_BASE_URL=https://your-app-name.vercel.app/api
VITE_APP_URL=https://your-app-name.vercel.app

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_REAL_TIME=true

# Security
VITE_ENABLE_HTTPS=true
VITE_SECURE_COOKIES=true

# Performance
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CACHING=true

# Third-party Integrations (Add your keys)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_key
STRIPE_SECRET_KEY=sk_live_your_stripe_secret
VITE_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
OPENAI_API_KEY=your_openai_api_key

# Email Service (if using external provider)
SENDGRID_API_KEY=your_sendgrid_api_key
SMTP_HOST=smtp.sendgrid.net
SMTP_PORT=587

# Redis (if using external Redis)
REDIS_URL=redis://your_redis_url

# Monitoring
SENTRY_DSN=your_sentry_dsn
VERCEL_ANALYTICS_ID=your_vercel_analytics_id